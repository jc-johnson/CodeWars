
<-- brute force-->
function divisibleCount(x, y, k) {
   //code me
   var count = 0;

   for (i = x; i <= y; i++) {
      if (i % k === 0) count++;
   }

   return count;
}

<-- O(1) solution -->
function divisibleCount (x, y, k) {

    var low = getHigherDivisible(x, k);
    var high = getLowerDevisible(y, k);

    var totalDivisible = high - low;
    var total = totalDivisible / k;
    return total + 1;
}

function getHigherDivisible(x, k) {
    var temp = x;
    while (temp % k != 0) {
        temp++;
    }
    return temp;
}

function getLowerDevisible(x, k) {
    var temp = x;
    while (temp % k != 0) {
        temp--;
    }
    return temp;
}

